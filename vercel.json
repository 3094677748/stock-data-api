{
  "version": 2,
  "builds": [
    {
      "src": "api/health.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.9",
        "maxLambdaSize": "15mb",
        "suppressResponse": false
      }
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/api/health.py"
    },
    {
      "src": "/health",
      "dest": "/api/health.py"
    }
  ],
  "functions": {
    "api/health.py": {
      "maxDuration": 10,
      "memory": 1024
    }
  }
}